{
  "name": "@cobbl-ai/sdk",
  "version": "0.1.0",
  "description": "Cobbl SDK - Close the Loop on LLM Prompt Feedback",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./admin": {
      "import": {
        "types": "./dist/admin.d.mts",
        "default": "./dist/admin.mjs"
      },
      "require": {
        "types": "./dist/admin.d.ts",
        "default": "./dist/admin.js"
      }
    },
    "./public": {
      "import": {
        "types": "./dist/public.d.mts",
        "default": "./dist/public.mjs"
      },
      "require": {
        "types": "./dist/public.d.ts",
        "default": "./dist/public.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "admin": [
        "./dist/admin.d.ts"
      ],
      "public": [
        "./dist/public.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "pnpm jest",
    "test:watch": "pnpm jest --watch",
    "test:coverage": "pnpm jest --coverage",
    "prepublishOnly": "pnpm clean && pnpm build",
    "release": "bash scripts/release.sh"
  },
  "keywords": [
    "prompt",
    "llm",
    "ai",
    "feedback",
    "promptops",
    "sdk",
    "openai",
    "anthropic",
    "prompt-engineering"
  ],
  "author": "Cobbl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@jest/test-sequencer": "29.7.0",
    "@types/jest": "29.5.12",
    "@types/node": "^18.0.0",
    "jest": "29.7.0",
    "ts-jest": "^29.4.5",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
